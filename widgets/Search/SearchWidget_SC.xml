<?xml version="1.0" ?>
<configuration>
    <layers>
        <layer>
            <name>Parcels</name>
            <url>http://eos2/ArcGIS/rest/services/Parcel/AGS_parcel/MapServer/2</url>
            <expression>AIN like '[value]'</expression>
            <textsearchlabel>Search by Parcel ID  [ Example: 2861026900 or 2861%]</textsearchlabel>
            <titlefield>AIN</titlefield>
            <linkfield></linkfield>
            <fields all="false">
                <field name="AIN"/>
            </fields>
        </layer>
	<layer>
            <name>Address</name>
            <url>http://eos2/ArcGIS/rest/services/Parcel/AGS_parcel/MapServer/1</url>
            <expression>ST_NUM like '[value]'</expression>
            <textsearchlabel>Search by Street Number ... [ Example: 23920 or 239%]</textsearchlabel>
            <titlefield>ST_NUM</titlefield>
            <linkfield></linkfield>
            <fields all="True">
                <field name="ST_NUM"/>
		<field name="ST_NAME" Alias="Name"/>
            </fields>
        </layer>
	<layer>
            <name>Tract Number</name>
            <url>http://eos2/ArcGIS/rest/services/Parcel/AGS_parcel/MapServer/2</url>
            <expression>TRACT like '[value]'</expression>
            <textsearchlabel>Search by Tract ... [ Example: 33746 or 337%]</textsearchlabel>
            <titlefield>TRACT</titlefield>
            <linkfield></linkfield>
            <fields all="false">
                <field name="TRACT"/>
            </fields>
        </layer>
    </layers>
    <zoomscale>10000</zoomscale>
</configuration>

