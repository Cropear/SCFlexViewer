<configuration>
	<layers>
		<layer>
			<definitionexpression></definitionexpression>
			<enableexport>true</enableexport>
			<name>Parcels</name>
	     	<url>http://eos2/ArcGIS/rest/services/Parcel/AGS_parcel_bldg/MapServer/1</url>
			<expressions>
				<expression alias="Parcels" textsearchlabel="Search by Parcel [ Example: 2861026900 or 2861%]" field="AIN" usedomain="true">AIN like '[value]'</expression>
			</expressions>
			<graphicalsearchlabel>Use one of the graphical search tools to select Parcel</graphicalsearchlabel>
			<spatialsearchlayer>true</spatialsearchlayer>
			<titlefield>AIN</titlefield>
			 <fields all="false">
					<field name="OWNER" gridfield="true"/>
					<field name="ALLADD" gridfield="true"/>
					<field name="APN" gridfield="true"/>
	            </fields>
			<linkfield></linkfield>
			<icon isfield="false"></icon>
			<zoomscale usegeometry="true"/>
			<autoopendatagrid>true</autoopendatagrid>
		</layer>
		<layer>
			<definitionexpression></definitionexpression>
			<enableexport>true</enableexport>
			<name>Address</name>
	     	<url>http://eos2/ArcGIS/rest/services/Parcel/AGS_parcel_bldg/MapServer/1</url>
			<expressions>
				<expression alias="Parcels" textsearchlabel="Search by Street Number ... [ Example: 23920 or 239%]" field="STREETNO" usedomain="true">STREETNO like '[value]'</expression>
			</expressions>
			<graphicalsearchlabel>Use one of the graphical search tools to select Address</graphicalsearchlabel>
			<spatialsearchlayer>true</spatialsearchlayer>
			<titlefield>ALLADD</titlefield>
			 <fields all="false">
					<field name="OWNER" gridfield="true"/>
					<field name="ALLADD" gridfield="true"/>
					<field name="APN" gridfield="true"/>
	            </fields>
			<linkfield></linkfield>
			<icon isfield="false"></icon>
			<zoomscale usegeometry="true"/>
			<autoopendatagrid>true</autoopendatagrid>
		</layer>
		<layer>
			<definitionexpression></definitionexpression>
			<enableexport>true</enableexport>
			<name>Owner</name>
	     	<url>http://eos2/ArcGIS/rest/services/Parcel/AGS_parcel_bldg/MapServer/1</url>
			<expressions>
				<expression alias="Owner" textsearchlabel="Search by Owner ... [ Example: Santa Clarita ]" field="OWNER" usedomain="true">OWNER like '[value]%'</expression>
			</expressions>
			<graphicalsearchlabel>Use one of the graphical search tools to select Address</graphicalsearchlabel>
			<spatialsearchlayer>true</spatialsearchlayer>
			<titlefield>OWNER</titlefield>
			 <fields all="false">
					<field name="OWNER" gridfield="true"/>
					<field name="ALLADD" gridfield="true"/>
					<field name="APN" gridfield="true"/>
	            </fields>
			<linkfield></linkfield>
			<icon isfield="false"></icon>
			<zoomscale usegeometry="true"/>
			<autoopendatagrid>true</autoopendatagrid>
		</layer>
		<layer>
			<definitionexpression></definitionexpression>
			<enableexport>true</enableexport>
			<name>Tract</name>
	     	<url>http://eos2/ArcGIS/rest/services/Parcel/AGS_parcel_bldg/MapServer/1</url>
			<expressions>
				<expression alias="Tract" textsearchlabel="Search by Tract ... [ Example: 33746 or 337%]" field="TRACTNO" usedomain="true">TRACTNO like '[value]'</expression>
			</expressions>
			<graphicalsearchlabel>Use one of the graphical search tools to select Parcel</graphicalsearchlabel>
			<spatialsearchlayer>true</spatialsearchlayer>
			<titlefield>TRACTNO</titlefield>
			 <fields all="false">
                <field name="TRACTNO" gridfield="true"/>
				<field name="OWNER" gridfield="true"/>
				<field name="ALLADD" gridfield="true"/>
				<field name="APN" gridfield="true"/>
            </fields>
			<linkfield></linkfield>
			<icon isfield="false"></icon>
			<zoomscale usegeometry="true"/>
			<autoopendatagrid>true</autoopendatagrid>
		</layer>

